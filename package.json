{
  "name": "unmono",
  "version": "0.0.11",
  "type": "module",
  "module": "dist/index.js",
  "unmono": {
    "entry": ["src/index.ts","src/cli.ts"],
    "output": "dist-lib",
    "publicPath": "/",
    "documentPath": "./explame/",
    "documentBuildPath": "./doc-dist",
    "description": "unmono description",
    "title": "unmono",
    "dts": true,
    "tsconfig": "tsconfig.explame.json"
  },
  "bin": {
    "unmono": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup --config rollup.config.ts --configPlugin esbuild",
    "release": "pnpm build && release-npm ",
    "release-npm": "pnpm publish --registry=https://registry.npmjs.org/ --no-git-checks "
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.4",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "vite-plugin-dts": "^3.5.1",
    "vite-tsconfig-paths": "^4.2.1",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.4",
    "rollup": "^4.1.4",
    "less": "^4.2.0",
    "vite": "^4.4.6",
    "vue": "^3.3.7",
    "rollup-plugin-esbuild": "^6.1.0",
    "vitepress-library": "^1.0.4",
    "esbuild": "^0.19.5"
  }
}